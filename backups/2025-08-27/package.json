{
  "name": "fisioflow-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "check": "npm run lint && npm run type-check",
    "postinstall": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:reset": "prisma migrate reset --force && npm run prisma:seed",
    "db:setup": "prisma generate && prisma db push && npm run prisma:seed",
    "deploy": "node scripts/deploy.js",
    "health-check": "node scripts/health-check.js check",
    "health-monitor": "node scripts/health-check.js monitor",
    "health-report": "node scripts/health-check.js report",
    "backup": "node scripts/backup.js",
    "backup:schedule": "node scripts/backup.js schedule",
    "backup:clean": "node scripts/backup.js clean",
    "query:analyze": "node scripts/query-optimizer.js analyze",
    "query:optimize": "node scripts/query-optimizer.js optimize",
    "query:monitor": "node scripts/query-optimizer.js monitor",
    "env:setup": "node scripts/setup-env.js setup",
    "env:export": "node scripts/setup-env.js export",
    "env:validate": "node scripts/setup-env.js validate",
    "env:update-from-cli": "node scripts/update-env-from-cli.js",
    "env:setup-railway": "node scripts/setup-railway.js",
    "env:setup-neon": "node scripts/setup-neon.js",
    "env:auto-setup": "npm run env:setup-railway && npm run env:setup-neon",
    "env:verify-cli": "node scripts/update-env-from-cli.js --verify-only",
    "env:status": "node scripts/update-env-from-cli.js --status",
    "env:validate-config": "node scripts/validate-env-config.js",
    "env:test-connections": "node scripts/validate-env-config.js",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:api": "jest --selectProjects api",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "ci:test": "npm run lint && npm run type-check && npm run test",
    "ci:build": "npm run build",
    "ci:deploy": "npm run deploy",
    "release:patch": "npm version patch && git push origin main --tags",
    "release:minor": "npm version minor && git push origin main --tags",
    "release:major": "npm version major && git push origin main --tags",
    "release:prerelease": "npm version prerelease --preid=alpha && git push origin main --tags",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "docker:build": "docker build -t fisioflow .",
    "docker:run": "docker run -p 3000:3000 fisioflow",
    "railway:setup": "pwsh -ExecutionPolicy Bypass -File scripts/railway-setup.ps1",
    "railway:setup-force": "pwsh -ExecutionPolicy Bypass -File scripts/railway-setup.ps1 -Force",
    "railway:login": "railway login",
    "railway:deploy": "node scripts/railway-cli-deploy.js",
    "railway:deploy-quick": "railway up",
    "railway:deploy-production": "node scripts/railway-cli-deploy.js --environment production",
    "railway:deploy-staging": "node scripts/railway-cli-deploy.js --environment staging",
    "railway:logs": "railway logs --follow",
    "railway:logs-tail": "railway logs --tail 100",
    "railway:logs-error": "railway logs --filter error",
    "railway:status": "railway status",
    "railway:status-detailed": "railway status --json",
    "railway:vars": "railway variables",
    "railway:vars-set": "railway variables set",
    "railway:vars-delete": "railway variables delete",
    "railway:domains": "railway domain",
    "railway:domains-add": "railway domain add",
    "railway:rollback": "railway rollback",
    "railway:restart": "railway restart",
    "railway:shell": "railway shell",
    "railway:connect": "railway connect",
    "railway:whoami": "railway whoami",
    "railway:projects": "railway projects",
    "railway:link": "railway link",
    "railway:unlink": "railway unlink",
    "railway:open": "railway open",
    "railway:metrics": "railway metrics",
    "railway:health": "railway logs --filter health",
    "railway:validate": "node scripts/railway-cli-deploy.js --validate-only",
    "railway:backup-before-deploy": "npm run backup && npm run railway:deploy",
    "railway:full-deploy": "npm run check && npm run build && npm run railway:deploy",
    "mcp:validate": "node scripts/validate-mcp-config.js",
    "mcp:setup": "npm run mcp:validate && npm run env:validate",
    "mcp:test-railway": "node scripts/validate-mcp-config.js --test-railway",
    "mcp:test-neon": "node scripts/validate-mcp-config.js --test-neon",
    "mcp:health-check": "node scripts/validate-mcp-config.js --health-check",
    "neon:status": "node scripts/validate-mcp-config.js --neon-status",
    "neon:backup": "node scripts/validate-mcp-config.js --neon-backup",
    "neon:maintenance": "node scripts/validate-mcp-config.js --neon-maintenance",
    "monitor:health": "bash scripts/health-check.sh",
    "monitor:autofix": "bash scripts/auto-fix.sh",
    "monitor:diagnostic": "node scripts/diagnostic-framework.js",
    "monitor:diagnostic-category": "node scripts/diagnostic-framework.js --category",
    "monitor:autofix-system": "node scripts/auto-fix-system.js",
    "monitor:autofix-force": "node scripts/auto-fix-system.js --force",
    "monitor:autofix-category": "node scripts/auto-fix-system.js --category",
    "monitor:prevention-start": "node scripts/prevention-system.js start",
    "monitor:prevention-stop": "node scripts/prevention-system.js stop",
    "monitor:prevention-status": "node scripts/prevention-system.js status",
    "monitor:prevention-restart": "node scripts/prevention-system.js restart",
    "monitor:alerts": "node scripts/alert-system.js",
    "monitor:alerts-test": "node scripts/alert-system.js --test",
    "monitor:error-detection-start": "node scripts/error-detection.js start",
    "monitor:error-detection-stop": "node scripts/error-detection.js stop",
    "monitor:error-detection-status": "node scripts/error-detection.js status",
    "monitor:error-detection-report": "node scripts/error-detection.js report",
    "monitor:full-check": "npm run monitor:health && npm run monitor:diagnostic",
    "monitor:full-fix": "npm run monitor:diagnostic && npm run monitor:autofix-system",
    "monitor:start-all": "npm run monitor:prevention-start && npm run monitor:error-detection-start",
    "monitor:stop-all": "npm run monitor:prevention-stop && npm run monitor:error-detection-stop",
    "monitor:status-all": "npm run monitor:prevention-status && npm run monitor:error-detection-status",
    "monitor:emergency-fix": "npm run monitor:autofix-force && npm run monitor:full-check",
    "monitor:railway-check": "npm run monitor:diagnostic-category railway",
    "monitor:neon-check": "npm run monitor:diagnostic-category neondb",
    "monitor:typescript-check": "npm run monitor:diagnostic-category typescript",
    "monitor:railway-fix": "npm run monitor:autofix-category railway",
    "monitor:neon-fix": "npm run monitor:autofix-category neondb",
    "monitor:typescript-fix": "npm run monitor:autofix-category typescript"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.0",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "3.9.0",
    "@neondatabase/serverless": "^1.0.1",
    "@prisma/adapter-neon": "^6.14.0",
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
