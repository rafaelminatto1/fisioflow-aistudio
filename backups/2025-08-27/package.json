    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@types/bcryptjs": "^2.4.6",
    "async_hooks": "^1.0.0",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "chokidar": "^3.5.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "html2pdf.js": "^0.11.1",
    "lucide-react": "^0.408.0",
    "next": "^14.2.32",
    "next-auth": "^5.0.0-beta.29",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.14",
    "pg": "^8.16.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.1",
    "react-is": "^19.1.1",
    "recharts": "^3.1.2",
    "redis": "^4.7.0",
    "sonner": "^2.0.7",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.3.0",
